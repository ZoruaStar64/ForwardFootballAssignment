generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_CONNECTION_URL")
}

model Player_Info {
  player String @id
  dribble_skills Decimal
  length Int
  width Decimal
  age Float
  ball_control Int
  passing_under_pressure Decimal
  team String
  position String
}